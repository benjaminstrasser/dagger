{
  "name": "dagger",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/dagger/src",
  "projectType": "application",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "ts-node --esm --project=apps/dagger/tsconfig.app.json apps/dagger/src/main.ts --experimental-specifier-resolution=node"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/dagger/**/*.ts"]
      }
    }
  },
  "tags": []
}
